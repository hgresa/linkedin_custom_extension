<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="customer">
            <group id="create_account">
                <field id="linkedin_profile_field_option" translate="label" type="select" sortOrder="1000"
                       showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Linkedin Profile Field Option</label>
                    <source_model>Tomadevall\Developers\Model\Config\Source\LinkedinValues</source_model>
                    <backend_model>Tomadevall\Developers\Model\Config\Backend\Linkedin</backend_model>
                </field>
           </group>
        </section>
    </system>
</config>
